#!/usr/bin/expect

set user [lindex $argv 0]
set ip [lindex $argv 1]
set passwd [lindex $argv 2]
set EYCLUSTER_HOME [lindex $argv 3]
set daytime [exec date +%y%m%d%H%M%S]
set prompt "*${user}@*\\\$"
if { $user == "root" } {
        set prompt "*${user}@*#"
}
spawn ssh ${user}@${ip}
set timeout -1
expect {     
        -nocase "yes/no" {send "yes\n";exp_continue} 
        -nocase "password" {send "$passwd\n"}
        "${prompt}" {send "\r"}
}
expect "${prompt}"
send "ps -ef > ${EYCLUSTER_HOME}/bin/log/track_pid.info_${daytime}\r"
expect "${prompt}"
send "exit\r"
expect eof
